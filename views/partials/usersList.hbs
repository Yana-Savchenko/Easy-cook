  <table class="table table-bordered">
    <thead class="thead-light">
      <tr>
        <th scope="col">First name <span class="fas fa-sort-alpha-down"></span></th>
        <th scope="col">Last name <span class="fas fa-sort-alpha-down"></span></th>
        <th scope="col">Email <span class="fas fa-sort-alpha-down"></span></th>
        <th scope="col">Age <span class="fas fa-sort-numeric-up"></span></th>
      </tr>
    </thead>
    {{#each users}}
    <tr>
      <td>{{this.firstName}}</td>
      <td>{{this.lastName}}</td>
      <td>{{this.email}}</td>
      <td>{{this.age}}</td>
    </tr>
    {{/each}}
  </table>
  <nav>
    <ul class="pagination">
      <li class="page-item disabled">
        <a class="page-link" href="#" aria-label="Previous">
          <span aria-hidden="true">&laquo;</span>
          <span class="sr-only">Previous</span>
        </a>
      </li>
      {{#each pages}}
      <li class="page-item  {{#if this.isActive}}active{{/if}}" data-page={{this.content}}><a class="page-link" href="">{{this.content}}</a></li>
      {{/each}}
      <li class="page-item disabled">
        <a class="page-link" href="#" aria-label="Next">
          <span aria-hidden="true">&raquo;</span>
          <span class="sr-only">Next</span>
        </a>
      </li>
    </ul>
  </nav>